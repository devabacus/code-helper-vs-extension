{
  "name": "code-helper",
  "displayName": "code-helper",
  "publisher": "mrfrolk",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/devabacus/code-helper-vs-extension.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:dart"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "codeHelperTreeView",
          "name": "Code Helper"
        }
      ]
    },
    "commands": [
      {
        "command": "code-helper.snippet_generate",
        "title": "snippet_generate"
      },
      {
        "command": "code-helper.pre_snippet_generate",
        "title": "pre_snippet_generate"
      },
      {
        "command": "code-helper.add_folders",
        "title": "add folders"
      },
      {
        "command": "code-helper.terminal",
        "title": "write to terminal"
      },
      {
        "command": "code-helper.myFlutter",
        "title": "myFlutter"
      },
      {
        "command": "code-helper.vsCodeExtHandler",
        "title": "myExt"
      },
      {
        "command": "code-helper.myPython",
        "title": "myPython"
      }
    ],
    "keybindings": [
      {
        "command": "code-helper.snippet_generate",
        "key": "alt+ctrl+shift+p"
      },
      {
        "command": "code-helper.pre_snippet_generate",
        "key": "alt+ctrl+shift+i"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.40",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1"
  }
}
