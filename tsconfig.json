{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@utils/*": ["src/utils/*"],
			"@utils": ["src/utils"],

			"@flutter_utils/*": ["src/flutter_utils/*"],
			"@flutter_utils": ["src/flutter_utils"],

			"@ui/*": ["src/ui/*"],
			"@ui": ["src/ui"],
			
			
		},
		"module": "Node16",
		"target": "ES2022",
		"outDir": "out",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		
	}
}
